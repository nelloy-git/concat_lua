{
    "Lua.workspace.ignoreDir": [
        ".vscode",
        "dst",
    ],
    "Lua.workspace.library": {
        "./lib": true,
        //"./src": true
    },
    "Lua.workspace.preloadFileSize": 2000,
    "Lua.diagnostics.disable": [
        "lowercase-global"
    ],
    "Lua.diagnostics.globals": [
        "Compiletime",
        "IsCompiletime",
        "GetSrcDir",
        "GetDstDir",
        "CompileFinal",
        "CompiletimeFinalToRuntime",
    ],
    "Lua.develop.enable": false,
    "Lua.completion.enable": true,
    "files.associations": {
        "chrono": "cpp"
    },
    "Lua.awakened.cat": true,
    "Lua.workspace.useGitIgnore": false,
}
